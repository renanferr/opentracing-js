{
  "name": "apm-opentracing-ipp",
  "version": "0.0.1-alpha",
  "description": "",
  "main": "./lib/index.js",
  "types": "./lib/",
  "scripts": {
    "test": "node_modules/mocha/bin/_mocha test/*.test.js - - -R spec",
    "build": "tsc",
    "build:test": "npm run build && npm run test",
    "build:watch": "tsc --watch",
    "docs": "typedoc --out docs src",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "cover": "node_modules/.bin/nyc --silent npm test && npm run cover:report",
    "cover:report": "node_modules/.bin/nyc report --reporter=lcov --reporter=text",
    "ci": "npm run build && npm run cover",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run ci",
    "preversion": "npm run ci"
  },
  "keywords": [
    "apm",
    "opentracing",
    "application monitoring",
    "elastic-apm"
  ],
  "author": "Renan Ferreira",
  "repository": {
    "type": "git",
    "url": "https://github.com/renanferr/apm-opentracing-ipp"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/elastic-apm-node": "^2.7.0",
    "@types/express": "^4.17.1",
    "@types/istanbul": "^0.4.30",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "express": "^4.17.1",
    "gh-pages": "^2.1.1",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2",
    "tslint": "^5.19.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "elastic-apm-node": "^2.16.0",
    "elastic-apm-node-opentracing": "^1.1.0",
    "opentracing": "^0.14.4"
  }
}
